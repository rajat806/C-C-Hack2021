// Next Greatest Element from right to left
#include<bits/stdc++.h>
using namespace std;

int nge(int a[],int n){
    stack<int> st; 
    int nge[n]; 
    for(int i=0; i<n; ++i){

        while(!st.empty() && st.top() <=a[i])
            st.pop();
        

        if(i < n){
            if(st.empty() == false)
                 nge[i] = st.top();
               
            else
                nge[i]=-1; 
              
        }
        st.push(a[i]);
    }
    for(int i=0; i<n; ++i){
        cout << nge[i] << " ";
    }
    return 0;

}

int main(){
   
    int n;
    cout << "Enter size";
    cin >> n;
    int a[n];
    for(int i=0; i<n ;++i){
        cin >> a[i];
    }
    nge(a,n);
}


